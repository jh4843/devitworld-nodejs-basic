  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "description": "node.js basic tutorial repository",
  "dependencies": {
    "express": "^4.19.2",
    "https": "^1.0.0",
    "ws": "^8.18.0"
  },
  "devDependencies": {
    "@asyncapi/generator": "^1.15.0",
    "@asyncapi/html-template": "^2.3.9",
    "@types/chai": "^4.3.16",
    "@types/express": "^4.17.21",
    "@types/mocha": "^10.0.7",
    "@types/node": "^20.14.12",
    "@types/ws": "^8.5.11",
    "chai": "^5.1.1",
    "mocha": "^10.7.0",
    "nodemon": "^3.1.4",
    "pkg": "^5.8.1",
    "ts-node": "^10.9.2",
    "tsx": "^4.16.2",
    "typescript": "^5.5.4"
  }
