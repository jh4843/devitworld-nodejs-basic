{
  "name": "vx.web.app",
  "version": "1.0.0",
  "main": "index.js",
  "bin": "dist/bundle.js",
  "scripts": {
    "build": "webpack --config webpack.prod.js",
    "dev": "webpack serve --config webpack.dev.js",
    "start:dev": "concurrently \"npm run start\" \"npm run dev\"",
    "test": "mocha --timeout 10000 --require ts-node/register test/**/*.spec.ts --exit",
    "start": "nodemon --exec tsx src/app.ts",
    "pkg": "pkg . --out-path ./bin",
    "generate:asyncapi": "npx ag asyncapi/asyncapi.yaml @asyncapi/html-template -o public/asyncapi-docs --force-write"
  },
  "pkg": {
    "scripts": "dist/**/*.js",
    "assets": [
      "assets/**/*",
      "public/**/*"
    ],
    "targets": [
      "node16-win-x64",
      "node16-linux-x64",
      "node16-macos-x64"
    ]
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "description": "node.js basic tutorial repository",
  "dependencies": {
    "app-root-path": "^3.1.0",
    "bufferutil": "^4.0.8",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.1",
    "express": "^4.19.2",
    "https": "^1.0.0",
    "reflect-metadata": "^0.2.2",
    "sqlite3": "^5.1.7",
    "typeorm": "^0.3.20",
    "utf-8-validate": "^5.0.10",
    "ws": "^8.18.0"
  },
  "devDependencies": {
    "@asyncapi/generator": "^1.15.0",
    "@asyncapi/html-template": "^2.3.9",
    "@types/chai": "^4.3.16",
    "@types/express": "^4.17.21",
    "@types/mocha": "^10.0.7",
    "@types/node": "^20.14.12",
    "@types/ws": "^8.5.11",
    "chai": "^5.1.1",
    "concurrently": "^8.2.2",
    "mocha": "^10.7.0",
    "nodemon": "^3.1.4",
    "pkg": "^5.8.1",
    "ts-loader": "^9.5.1",
    "ts-node": "^10.9.2",
    "tsx": "^4.16.2",
    "typescript": "^5.5.4",
    "webpack": "^5.93.0",
    "webpack-cli": "^5.1.4",
    "webpack-dev-server": "^5.0.4",
    "webpack-node-externals": "^3.0.0"
  }
}
